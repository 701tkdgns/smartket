<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.project">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AppOnpage.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".PagerSlidingTabStrip.ViewPagerActivity" />
        <activity android:name=".PagerSlidingTabStrip.PageFragment" />
        <activity android:name=".fragment.FragmentActivity" />
        <activity android:name=".User.user_login_fragment" />
        <activity android:name=".RecyclerViewActivity.RecyclerViewActivity" />
        <activity android:name=".MainNavigation.MainNavigationActivity">
        </activity>
    </application>

</manifest>